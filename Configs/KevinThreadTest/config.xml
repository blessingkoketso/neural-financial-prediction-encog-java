<!-- Temporal dataset -->
	<entry key="NORMALIZED_LOW">0</entry>
	<entry key="NORMALIZED_HIGH">1.0</entry>
	<entry key="INPUT_WINDOW_SIZE">12</entry>
	<entry key="PREDICT_WINDOW_SIZE">1</entry>

<!-- Training -->
	<!-- Model Options -->
		<!-- Regression -->
		<!-- Classification -->
		<entry key="PREDICT_MODEL">Regression</entry>
	<entry key="ACTIVATION_FUNCTION">?:B->SIGMOID->25:B->SIGMOID->?</entry>
